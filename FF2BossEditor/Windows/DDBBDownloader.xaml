<Window x:Class="FF2BossEditor.Windows.DDBBDownloader"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FF2BossEditor.Windows"
        mc:Ignorable="d"
        Height="100" Width="550"
        Title="DataBase Downloader"
        ResizeMode="NoResize"
        Loaded="DDBBDownloader_Loaded">
    
    <Grid>
        <ProgressBar x:Name="DownloadBar" Minimum="0" Maximum="100"/>
    </Grid>
</Window>
